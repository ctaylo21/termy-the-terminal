{
  "name": "termy-the-terminal",
  "version": "0.8.0",
  "description": "Web-based terminal powered by React",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ctaylo21/termy-the-terminal"
  },
  "bugs": {
    "url": "https://github.com/ctaylo21/termy-the-terminal/issues"
  },
  "scripts": {
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "start": "rollup -c -w",
    "build": "rollup -c",
    "commit": "git-cz",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "prepublishOnly": "npm test && npm run lint",
    "lint": "eslint src --ext ts,tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Caleb Taylor <caleb89taylor@gmail.com>",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@svgr/rollup": "^4.3.2",
    "@testing-library/react": "^8.0.7",
    "@types/jest": "^24.0.15",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/user-event": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.5",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^22.14.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-css-modules": "^2.1.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "npm": "^6.10.2",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.17.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-scss": "^1.0.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "rollup-plugin-url": "^2.2.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
