{
  "name": "termy-the-terminal",
  "version": "1.0.0",
  "description": "Web-based terminal powered by React",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ctaylo21/termy-the-terminal"
  },
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --open --mode=development",
    "build": "webpack --mode=production",
    "predeploygh": "npm run build",
    "deploygh": "gh-pages -d dist",
    "commit": "git-cz",
    "lint": "tslint --project tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "tslint"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Caleb Taylor <caleb89taylor@gmail.com>",
  "dependencies": {
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/user-event": "^1.4.0",
    "clean-webpack-plugin": "^1.0.1",
    "commitizen": "^3.0.5",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "prettier": "1.16.4",
    "react-testing-library": "^5.8.0",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
