{
  "name": "termy-the-terminal",
  "version": "1.2.0",
  "description": "Web-based terminal powered by React",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "react",
    "terminal",
    "typescript",
    "react-component"
  ],
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ctaylo21/termy-the-terminal"
  },
  "bugs": {
    "url": "https://github.com/ctaylo21/termy-the-terminal/issues"
  },
  "scripts": {
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "start": "rollup -c rollup.dev.config.js -w",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "build:demo": "rollup -c rollup.dev.config.js --configDocBuild",
    "commit": "git-cz",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "prepublishOnly": "npm test && npm run lint",
    "lint": "eslint src --ext ts,tsx",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "author": "Caleb Taylor <caleb89taylor@gmail.com>",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@rollup/plugin-url": "^4.0.2",
    "@svgr/rollup": "^5.3.1",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.2",
    "@types/jest": "^25.2.1",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^13.13.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "commitizen": "^4.0.4",
    "coveralls": "^3.0.11",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-css-modules": "^2.1.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^10.1.4",
    "node-sass": "^4.14.1",
    "npm": "^6.14.4",
    "prettier": "^2.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "release-it": "^13.6.1",
    "rollup": "^2.6.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^2.4.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.4.0",
    "typescript": "3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
