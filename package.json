{
  "name": "termy-the-terminal",
  "version": "1.0.2",
  "description": "Web-based terminal powered by React",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "react",
    "terminal",
    "typescript",
    "react-component"
  ],
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ctaylo21/termy-the-terminal"
  },
  "bugs": {
    "url": "https://github.com/ctaylo21/termy-the-terminal/issues"
  },
  "scripts": {
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "start": "rollup -c rollup.dev.config.js -w",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "build:demo": "rollup -c rollup.dev.config.js --configDocBuild",
    "commit": "git-cz",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "prepublishOnly": "npm test && npm run lint",
    "lint": "eslint src --ext ts,tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Caleb Taylor <caleb89taylor@gmail.com>",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@svgr/rollup": "^4.3.3",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.22",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.7",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.0",
    "npm": "^6.13.0",
    "prettier": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.26.4",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript2": "^0.25.2",
    "rollup-plugin-url": "^3.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
