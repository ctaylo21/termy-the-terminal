{
  "name": "termy-the-terminal",
  "version": "0.5.2",
  "description": "Web-based terminal powered by React",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ctaylo21/termy-the-terminal"
  },
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --collect-coverage",
    "start": "webpack-dev-server --open --mode=development",
    "build": "webpack --mode=production",
    "commit": "git-cz",
    "lint": "eslint src --ext ts,tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Caleb Taylor <caleb89taylor@gmail.com>",
  "dependencies": {
    "@types/jest": "^24.0.13",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "lodash-es": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "@types/user-event": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "clean-webpack-plugin": "^2.0.2",
    "commitizen": "^3.1.1",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jest": "^22.6.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-css-modules": "^2.0.0",
    "jest-dom": "^3.2.2",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "react-testing-library": "^7.0.1",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.1.0",
    "svg-react-loader": "^0.4.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.4.5",
    "webpack": "^4.32.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
